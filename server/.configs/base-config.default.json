{
  "apiBaseUrl": "http://localhost:3000/api",
  "appSecretKey": "set-your-app-secret-key",
  "clientToken": {
    "scopes": ["user.read", "user.write", "user.account.read", "user.account.write", "post.read", "post.write", "storage.read", "storage.write"]
  },
  "hostToken": {
    "scopes": ["user.read", "app.read", "app.host", "auth.host", "user.create", "user.delete"],
    "accessToken": "set-your-access-token"
  },
  "recaptcha": {
    "enable": false,
    "siteKey": "set-your-recaptcha-site-key",
    "secretKey": "set-your-recaptcha-secret-key"
  }
}
