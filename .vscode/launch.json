{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "start server",
			"console": "integratedTerminal",
			"program": "${workspaceFolder}/server/built/index.js",
			"outFiles": [
				"${workspaceFolder}/**/*.js"
			]
		},
		{
			"type": "node",
			"request": "launch",
			"name": "start setup of server",
			"console": "integratedTerminal",
			"program": "${workspaceFolder}/server/built/index.js",
			"args": [
				"--setup"
			],
			"outFiles": [
				"${workspaceFolder}/**/*.js"
			]
		}
	]
}
